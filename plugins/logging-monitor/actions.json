        "cmd": "gcloud bigquery tables create {tabla} --dataset={dataset} --schema='{schema}'",
        "params": [
          "tabla",
          "dataset",
          "schema"
        ],
        "param_hints": {}
      }
    ],
    "📝 Modificación": [
      {
        "cmd": "gcloud bigquery tables update {tabla} --dataset={dataset} --description='{descripcion}'",
        "params": [
          "tabla",
          "dataset",
          "descripcion"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery tables update {tabla} --dataset={dataset} --expiration={expiration_time}",
        "params": [
          "tabla",
          "dataset",
          "expiration_time"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery tables update {tabla} --dataset={dataset} --labels='{labels}'",
        "params": [
          "tabla",
          "dataset",
          "labels"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery tables update {tabla} --dataset={dataset} --time-partitioning-type={tipo_particion}",
        "params": [
          "tabla",
          "dataset",
          "tipo_particion"
        ],
        "param_hints": {}
      }
    ],
    "❌ Eliminación": [
      {
        "cmd": "gcloud bigquery tables delete {tabla} --dataset={dataset} --quiet",
        "params": [
          "tabla",
          "dataset"
        ],
        "param_hints": {}
      }
    ],
    "🔐 Seguridad": [
      {
        "cmd": "gcloud bigquery tables add-iam-policy-binding {tabla} --dataset={dataset} --member='{miembro}' --role='{rol}'",
        "params": [
          "tabla",
          "dataset",
          "miembro",
          "rol"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery tables remove-iam-policy-binding {tabla} --dataset={dataset} --member='{miembro}' --role='{rol}'",
        "params": [
          "tabla",
          "dataset",
          "miembro",
          "rol"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery tables get-iam-policy {tabla} --dataset={dataset}",
        "params": [
          "tabla",
          "dataset"
        ],
        "param_hints": {}
      }
    ]
  },
  "Jobs": {
    "⚡ Jobs/Procesamiento": [
      {
        "cmd": "gcloud bigquery jobs run {job_id} --query='{query}' --use_legacy_sql=false",
        "params": [
          "job_id",
          "query"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery jobs cancel {job_id}",
        "params": [
          "job_id"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery jobs describe {job_id}",
        "params": [
          "job_id"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery jobs list --project={proyecto}",
        "params": [
          "proyecto"
        ],
        "param_hints": {}
      }
    ]
  },
  "Buckets": {
    "📤 Creación": [
      {
        "cmd": "gsutil mb -p {proyecto} -c {clase} -l {region} gs://{bucket}",
        "params": [
          "proyecto",
          "clase",
          "region",
          "bucket"
        ],
        "param_hints": {
          "region": {
            "type": "combo",
            "ref": "regions"
          }
        }
      }
    ],
    "📝 Modificación": [
      {
        "cmd": "gsutil defacl set {acl} gs://{bucket}",
        "params": [
          "acl",
          "bucket"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gsutil lifecycle set {lifecycle_file} gs://{bucket}",
        "params": [
          "lifecycle_file",
          "bucket"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gsutil versioning set {estado} gs://{bucket}",
        "params": [
          "estado",
          "bucket"
        ],
        "param_hints": {}
      }
    ],
    "❌ Eliminación": [
      {
        "cmd": "gsutil rm -r gs://{bucket}",
        "params": [
          "bucket"
        ],
        "param_hints": {}
      }
    ],
    "🔐 Seguridad": [
      {
        "cmd": "gsutil iam ch {miembro}:{rol} gs://{bucket}",
        "params": [
          "miembro",
          "rol",
          "bucket"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gsutil iam ch -d {miembro}:{rol} gs://{bucket}",
        "params": [
          "miembro",
          "rol",
          "bucket"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gsutil iam get gs://{bucket}",
        "params": [
          "bucket"
        ],
        "param_hints": {}
      }
    ]
  },
