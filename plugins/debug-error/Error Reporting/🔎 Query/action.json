{
  "List error groups": {
    "cmd": "gcloud error-reporting groups list --project={project_id}",
    "params": [
      "project_id"
    ],
    "param_hints": {}
  },
  "List error events": {
    "cmd": "gcloud error-reporting events list --group={group_id} --project={project_id}",
    "params": [
      "group_id",
      "project_id"
    ],
    "param_hints": {}
  },
  "Describe error group": {
    "cmd": "gcloud error-reporting groups describe {group_id} --project={project_id}",
    "params": [
      "group_id",
      "project_id"
    ],
    "param_hints": {}
  }
}