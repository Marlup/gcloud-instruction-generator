{
  "Listar vistas": {
    "cmd": "gcloud logging views list --bucket={bucket_id} --location={location} --project={project}",
    "params": [
      "bucket_id",
      "location",
      "project"
    ],
    "param_hints": {}
  },
  "Describir vista": {
    "cmd": "gcloud logging views describe {view_id} --bucket={bucket_id} --location={location} --project={project}",
    "params": [
      "view_id",
      "bucket_id",
      "location",
      "project"
    ],
    "param_hints": {}
  }
}