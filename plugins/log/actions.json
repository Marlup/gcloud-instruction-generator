{
  "Log Entries": {
    "üîé Consulta": {
      "Leer entradas de logs": {
        "cmd": "gcloud logging read \"{filter}\" --limit={limit} --project={project}",
        "params": ["filter", "limit", "project"],
        "param_hints": {}
      },
      "Tail en tiempo real": {
        "cmd": "gcloud logging tail \"{filter}\" --project={project}",
        "params": ["filter", "project"],
        "param_hints": {}
      },
      "Listar logs": {
        "cmd": "gcloud logging logs list --project={project}",
        "params": ["project"],
        "param_hints": {}
      }
    },
    "üì§ Creaci√≥n": {
      "Escribir entrada de log simple": {
        "cmd": "gcloud logging write {log_id} \"{message}\" --severity={severity} --project={project}",
        "params": ["log_id", "message", "severity", "project"],
        "param_hints": {}
      }
    },
    "‚ùå Eliminaci√≥n": {
      "Borrar log": {
        "cmd": "gcloud logging logs delete {log_id} --project={project}",
        "params": ["log_id", "project"],
        "param_hints": {}
      }
    }
  },

  "Log Buckets": {
    "üîé Consulta": {
      "Listar buckets": {
        "cmd": "gcloud logging buckets list --location={location} --project={project}",
        "params": ["location", "project"],
        "param_hints": {}
      },
      "Describir bucket": {
        "cmd": "gcloud logging buckets describe {bucket_id} --location={location} --project={project}",
        "params": ["bucket_id", "location", "project"],
        "param_hints": {}
      }
    },
    "üì§ Creaci√≥n": {
      "Crear bucket": {
        "cmd": "gcloud logging buckets create {bucket_id} --location={location} --retention-days={days} --project={project}",
        "params": ["bucket_id", "location", "days", "project"],
        "param_hints": {}
      }
    },
    "üìù Modificaci√≥n": {
      "Actualizar bucket": {
        "cmd": "gcloud logging buckets update {bucket_id} --location={location} --retention-days={days} --project={project}",
        "params": ["bucket_id", "location", "days", "project"],
        "param_hints": {}
      }
    },
    "‚ùå Eliminaci√≥n": {
      "Borrar bucket": {
        "cmd": "gcloud logging buckets delete {bucket_id} --location={location} --project={project}",
        "params": ["bucket_id", "location", "project"],
        "param_hints": {}
      }
    },
    "üîê Seguridad": {
      "Get IAM bucket": {
        "cmd": "gcloud logging buckets get-iam-policy {bucket_id} --location={location} --project={project}",
        "params": ["bucket_id", "location", "project"],
        "param_hints": {}
      },
      "Set IAM bucket": {
        "cmd": "gcloud logging buckets set-iam-policy {bucket_id} {policy_file} --location={location} --project={project}",
        "params": ["bucket_id", "policy_file", "location", "project"],
        "param_hints": {
          "policy_file": {"type": "file", "extensions": [".json", ".yaml", ".yml"]}
        }
      }
    }
  },

