{
  "Adds IAM policy binding.": {
    "cmd": "gcloud projects add-iam-policy-binding {project_id} --member {member} --role {role}",
    "params": [
      "project_id",
      "member",
      "role"
    ],
    "param_hints": {}
  },
  "Removes IAM policy binding.": {
    "cmd": "gcloud projects remove-iam-policy-binding {project_id} --member {member} --role {role}",
    "params": [
      "project_id",
      "member",
      "role"
    ],
    "param_hints": {}
  }
}