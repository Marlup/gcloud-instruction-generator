{
  "Updates an existing resource.": {
    "cmd": "gcloud pubsub topics update {topic} --message-retention-duration={duration}",
    "params": [
      "topic",
      "duration"
    ],
    "param_hints": {}
  },
  "Updates an existing resource. (variant)": {
    "cmd": "gcloud pubsub topics update {topic} --labels={labels}",
    "params": [
      "topic",
      "labels"
    ],
    "param_hints": {}
  }
}