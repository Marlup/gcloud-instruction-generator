{
  "Publishes a message to a topic.": {
    "cmd": "gcloud pubsub topics publish {topic} --message={message}",
    "params": [
      "topic",
      "message"
    ],
    "param_hints": {}
  },
  "Pulls messages from a subscription.": {
    "cmd": "gcloud pubsub subscriptions pull {subscription} --limit={limit} --auto-ack",
    "params": [
      "subscription",
      "limit"
    ],
    "param_hints": {}
  },
  "Pulls messages from a subscription. (variant)": {
    "cmd": "gcloud pubsub subscriptions pull {subscription} --limit={limit}",
    "params": [
      "subscription",
      "limit"
    ],
    "param_hints": {}
  }
}