{
  "Borrar constraint de proyecto": {
    "cmd": "gcloud org-policies delete {constraint} --project={project}",
    "params": [
      "constraint",
      "project"
    ],
    "param_hints": {}
  },
  "Reset constraint a herencia": {
    "cmd": "gcloud org-policies reset {constraint} --project={project}",
    "params": [
      "constraint",
      "project"
    ],
    "param_hints": {}
  }
}