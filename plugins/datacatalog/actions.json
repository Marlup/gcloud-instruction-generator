  "Datasets": {
    "📤 Creación": [
      {
        "cmd": "gcloud data-catalog datasets create --dataset {dataset} --location {location}",
        "params": [
          "dataset",
          "location"
        ],
        "param_hints": {
          "location": {
            "type": "combo",
            "ref": "bigquery_locations"
          }
        }
      }
    ],
    "📝 Modificación": [
      {
        "cmd": "gcloud data-catalog datasets update --dataset {dataset} --description {description}",
        "params": [
          "dataset",
          "description"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud data-catalog datasets update --dataset {dataset} --labels {labels}",
        "params": [
          "dataset",
          "labels"
        ],
        "param_hints": {}
      }
    ],
    "❌ Eliminación": [
      {
        "cmd": "gcloud data-catalog datasets delete --dataset {dataset}",
        "params": [
          "dataset"
        ],
        "param_hints": {}
      }
    ],
    "🔐 Seguridad": [
      {
        "cmd": "gcloud data-catalog datasets add-iam-policy-binding --dataset {dataset} --member {member} --role {role}",
        "params": [
          "dataset",
