{
  "Datasets": {
    "📤 Creación": [
      {
        "cmd": "gcloud bigquery datasets create {dataset} --location={region}",
        "params": [
          "dataset",
          "region"
        ],
        "param_hints": {
          "region": {
            "type": "combo",
            "ref": "regions"
          }
        }
      }
    ],
    "📝 Modificación": [
      {
        "cmd": "gcloud bigquery datasets update {dataset} --description={descripcion}",
        "params": [
          "dataset",
          "descripcion"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery datasets update {dataset} --default-table-expiration={expiration}",
        "params": [
          "dataset",
          "expiration"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery datasets update {dataset} --labels={etiquetas}",
        "params": [
          "dataset",
          "etiquetas"
        ],
        "param_hints": {}
      }
    ],
    "❌ Eliminación": [
      {
        "cmd": "gcloud bigquery datasets delete {dataset} --quiet",
        "params": [
          "dataset"
        ],
        "param_hints": {}
      }
    ],
    "🔐 Seguridad": [
      {
        "cmd": "gcloud bigquery datasets add-iam-policy-binding {dataset} --member={miembro} --role={rol}",
        "params": [
          "dataset",
          "miembro",
          "rol"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery datasets remove-iam-policy-binding {dataset} --member={miembro} --role={rol}",
        "params": [
          "dataset",
          "miembro",
          "rol"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery datasets get-iam-policy {dataset}",
        "params": [
          "dataset"
        ],
        "param_hints": {}
      }
    ]
  },
