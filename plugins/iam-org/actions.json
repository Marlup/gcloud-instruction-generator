  "Buckets": {
    "📤 Creación": [
      {
        "cmd": "gsutil mb -p {proyecto} -l {region} gs://{bucket}",
        "params": [
          "proyecto",
          "region",
          "bucket"
        ],
        "param_hints": {
          "region": {
            "type": "combo",
            "ref": "regions"
          }
        }
      }
    ],
    "📝 Modificación": [
      {
        "cmd": "gsutil defacl set {acl} gs://{bucket}",
        "params": [
          "acl",
          "bucket"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gsutil lifecycle set {lifecycle_file} gs://{bucket}",
        "params": [
          "lifecycle_file",
          "bucket"
        ],
        "param_hints": {}
      }
    ],
    "❌ Eliminación": [
      {
        "cmd": "gsutil rm -r gs://{bucket}",
        "params": [
          "bucket"
        ],
        "param_hints": {}
      }
    ],
    "🔐 Seguridad": [
      {
        "cmd": "gsutil iam ch {miembro}:{rol} gs://{bucket}",
        "params": [
          "miembro",
          "rol",
          "bucket"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gsutil iam get gs://{bucket}",
        "params": [
          "bucket"
        ],
        "param_hints": {}
      }
    ]
  },
