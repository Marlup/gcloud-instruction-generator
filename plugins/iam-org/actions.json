  "Jobs": {
    "⚡ Jobs/Procesamiento": [
      {
        "cmd": "gcloud bigquery jobs run {job_id} --query={consulta} --use_legacy_sql={uso_legacy_sql}",
        "params": [
          "job_id",
          "consulta",
          "uso_legacy_sql"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery jobs cancel {job_id}",
        "params": [
          "job_id"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud bigquery jobs get {job_id}",
        "params": [
          "job_id"
        ],
        "param_hints": {}
      }
    ]
