{
  "Datasets": {
    "📤 Creación": [
      {
        "cmd": "gcloud dataplex datasets create --lake={lake} --dataset-id={dataset_id} --display-name={display_name} --description={description}",
        "params": [
          "lake",
          "dataset_id",
          "display_name",
          "description"
        ],
        "param_hints": {}
      }
    ],
    "📝 Modificación": [
      {
        "cmd": "gcloud dataplex datasets update --lake={lake} --dataset-id={dataset_id} --display-name={display_name} --description={description}",
        "params": [
          "lake",
          "dataset_id",
          "display_name",
          "description"
        ],
        "param_hints": {}
      }
    ],
    "❌ Eliminación": [
      {
        "cmd": "gcloud dataplex datasets delete --lake={lake} --dataset-id={dataset_id}",
        "params": [
          "lake",
          "dataset_id"
        ],
        "param_hints": {}
      }
    ],
    "🔐 Seguridad": [
      {
        "cmd": "gcloud dataplex datasets add-iam-policy-binding --lake={lake} --dataset-id={dataset_id} --member={member} --role={role}",
        "params": [
          "lake",
          "dataset_id",
          "member",
          "role"
        ],
        "param_hints": {}
      },
      {
        "cmd": "gcloud dataplex datasets remove-iam-policy-binding --lake={lake} --dataset-id={dataset_id} --member={member} --role={role}",
