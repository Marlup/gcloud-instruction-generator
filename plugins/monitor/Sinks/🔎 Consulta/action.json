{
  "Listar sinks": {
    "cmd": "gcloud logging sinks list --project={project}",
    "params": [
      "project"
    ],
    "param_hints": {}
  },
  "Describir sink": {
    "cmd": "gcloud logging sinks describe {sink_name} --project={project}",
    "params": [
      "sink_name",
      "project"
    ],
    "param_hints": {}
  }
}