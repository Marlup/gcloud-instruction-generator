{
  "Actualizar sink": {
    "cmd": "gcloud logging sinks update {sink_name} --log-filter=\"{filter}\" --project={project}",
    "params": [
      "sink_name",
      "filter",
      "project"
    ],
    "param_hints": {}
  },
  "Actualizar sink (children)": {
    "cmd": "gcloud logging sinks update {sink_name} --include-children --project={project}",
    "params": [
      "sink_name",
      "project"
    ],
    "param_hints": {}
  },
