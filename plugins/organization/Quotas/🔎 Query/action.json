{
  "List project quotas": {
    "cmd": "gcloud compute project-info describe --project={project_id}",
    "params": [
      "project_id"
    ],
    "param_hints": {}
  },
  "Describe service quota": {
    "cmd": "gcloud services quota list --service={service} --project={project_id}",
    "params": [
      "service",
      "project_id"
    ],
    "param_hints": {}
  }
}