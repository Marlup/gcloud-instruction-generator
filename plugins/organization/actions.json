{
  "Projects": {
    "🔎 Query": {
      "List projects": {
        "cmd": "gcloud projects list",
        "params": [],
        "param_hints": {}
      },
      "Describe project": {
        "cmd": "gcloud projects describe {project_id}",
        "params": ["project_id"],
        "param_hints": {}
      },
      "Get IAM policy (project)": {
        "cmd": "gcloud projects get-iam-policy {project_id}",
        "params": ["project_id"],
        "param_hints": {}
      }
    },
    "📤 Creation": {
      "Create project": {
        "cmd": "gcloud projects create {project_id} --name={name} --folder={folder_id}",
        "params": ["project_id", "name", "folder_id"],
        "param_hints": {}
      }
    },
    "📝 Modification": {
      "Move project to folder": {
        "cmd": "gcloud projects move {project_id} --folder={folder_id}",
        "params": ["project_id", "folder_id"],
        "param_hints": {}
      },
      "Move project to organization": {
        "cmd": "gcloud projects move {project_id} --organization={org_id}",
        "params": ["project_id", "org_id"],
        "param_hints": {}
      },
      "Update project metadata": {
        "cmd": "gcloud projects update {project_id} --update-labels={labels}",
        "params": ["project_id", "labels"],
        "param_hints": {}
      },
      "Set IAM policy (project)": {
        "cmd": "gcloud projects set-iam-policy {project_id} {policy_file}",
        "params": ["project_id", "policy_file"],
        "param_hints": {
          "policy_file": {"type": "file", "extensions": [".json", ".yaml"]}
        }
      }
    },
    "❌ Deletion": {
      "Delete project": {
        "cmd": "gcloud projects delete {project_id}",
        "params": ["project_id"],
        "param_hints": {}
      },
      "Undelete project": {
        "cmd": "gcloud projects undelete {project_id}",
        "params": ["project_id"],
        "param_hints": {}
      }
    }
  },
  "Folders": {
    "🔎 Query": {
      "List folders": {
        "cmd": "gcloud resource-manager folders list",
        "params": [],
        "param_hints": {}
      },
      "Describe folder": {
        "cmd": "gcloud resource-manager folders describe {folder_id}",
        "params": ["folder_id"],
        "param_hints": {}
      },
      "Get IAM policy (folder)": {
        "cmd": "gcloud resource-manager folders get-iam-policy {folder_id}",
        "params": ["folder_id"],
        "param_hints": {}
      }
    },
    "📤 Creation": {
      "Create folder": {
        "cmd": "gcloud resource-manager folders create --display-name={name} --parent={parent}",
        "params": ["name", "parent"],
        "param_hints": {}
      }
    },
    "📝 Modification": {
      "Move folder": {
        "cmd": "gcloud resource-manager folders move {folder_id} --destination-folder={dest_folder_id}",
        "params": ["folder_id", "dest_folder_id"],
        "param_hints": {}
      },
      "Set IAM policy (folder)": {
        "cmd": "gcloud resource-manager folders set-iam-policy {folder_id} {policy_file}",
        "params": ["folder_id", "policy_file"],
        "param_hints": {
          "policy_file": {"type": "file", "extensions": [".json", ".yaml"]}
        }
      }
    },
    "❌ Deletion": {
      "Delete folder": {
        "cmd": "gcloud resource-manager folders delete {folder_id}",
        "params": ["folder_id"],
        "param_hints": {}
      }
    }
  },
  "Organizations": {
    "🔎 Query": {
      "List organizations": {
        "cmd": "gcloud organizations list",
        "params": [],
        "param_hints": {}
      },
      "Describe organization": {
        "cmd": "gcloud organizations describe {org_id}",
        "params": ["org_id"],
        "param_hints": {}
      },
      "Get IAM policy (org)": {
        "cmd": "gcloud organizations get-iam-policy {org_id}",
        "params": ["org_id"],
        "param_hints": {}
      }
    },
    "📝 Modification": {
      "Set IAM policy (org)": {
        "cmd": "gcloud organizations set-iam-policy {org_id} {policy_file}",
        "params": ["org_id", "policy_file"],
        "param_hints": {
          "policy_file": {"type": "file", "extensions": [".json", ".yaml"]}
        }
      }
    }
  },
  "Billing": {
    "🔎 Query": {
      "List billing accounts": {
        "cmd": "gcloud billing accounts list",
        "params": [],
        "param_hints": {}
      },
      "Describe billing account": {
        "cmd": "gcloud billing accounts describe {billing_account_id}",
        "params": ["billing_account_id"],
        "param_hints": {}
      },
      "List billing subaccounts": {
        "cmd": "gcloud billing accounts list --filter='masterAccountId={billing_account_id}'",
        "params": ["billing_account_id"],
        "param_hints": {}
      },
      "List project billing info": {
        "cmd": "gcloud billing projects describe {project_id}",
        "params": ["project_id"],
        "param_hints": {}
      }
    },
    "📝 Modification": {
      "Link billing account to project": {
        "cmd": "gcloud billing projects link {project_id} --billing-account={billing_account_id}",
        "params": ["project_id", "billing_account_id"],
        "param_hints": {}
      },
      "Unlink billing account from project": {
        "cmd": "gcloud billing projects unlink {project_id}",
        "params": ["project_id"],
        "param_hints": {}
      },
      "Create billing budget": {
        "cmd": "gcloud billing budgets create --billing-account={billing_account_id} --budget-file={budget_file}",
        "params": ["billing_account_id", "budget_file"],
        "param_hints": {
          "budget_file": {"type": "file", "extensions": [".json", ".yaml"]}
        }
      }
    }
  },
  "Organization Policies": {
    "🔎 Query": {
      "List org policies for project": {
        "cmd": "gcloud org-policies list --project={project_id}",
        "params": ["project_id"],
        "param_hints": {}
      },
      "List constraints": {
        "cmd": "gcloud org-policies list-constraints --project={project_id}",
        "params": ["project_id"],
        "param_hints": {}
      },
      "Describe org policy constraint": {
        "cmd": "gcloud org-policies describe {constraint} --project={project_id}",
        "params": ["constraint", "project_id"],
        "param_hints": {}
      }
    },
    "📝 Modification": {
      "Set org policy (project)": {
        "cmd": "gcloud org-policies set-policy {policy_file} --project={project_id}",
        "params": ["policy_file", "project_id"],
        "param_hints": {
          "policy_file": {"type": "file", "extensions": [".json", ".yaml"]}
        }
      },
      "Set org policy (folder)": {
        "cmd": "gcloud org-policies set-policy {policy_file} --folder={folder_id}",
        "params": ["policy_file", "folder_id"],
        "param_hints": {
          "policy_file": {"type": "file", "extensions": [".json", ".yaml"]}
        }
      },
      "Set org policy (organization)": {
        "cmd": "gcloud org-policies set-policy {policy_file} --organization={org_id}",
        "params": ["policy_file", "org_id"],
        "param_hints": {
          "policy_file": {"type": "file", "extensions": [".json", ".yaml"]}
        }
      },
      "Clear org policy": {
        "cmd": "gcloud org-policies clear {constraint} --project={project_id}",
        "params": ["constraint", "project_id"],
        "param_hints": {}
      }
    }
  },
  "Services": {
    "🔎 Query": {
      "List enabled services": {
        "cmd": "gcloud services list --enabled --project={project_id}",
        "params": ["project_id"],
        "param_hints": {}
      },
      "List disabled services": {
        "cmd": "gcloud services list --available --project={project_id}",
        "params": ["project_id"],
        "param_hints": {}
      }
    },
    "📝 Modification": {
      "Enable service API": {
        "cmd": "gcloud services enable {service} --project={project_id}",
        "params": ["service", "project_id"],
        "param_hints": {}
      },
      "Disable service API": {
        "cmd": "gcloud services disable {service} --project={project_id}",
        "params": ["service", "project_id"],
        "param_hints": {}
      }
    }
  },
  "Quotas": {
    "🔎 Query": {
      "List project quotas": {
        "cmd": "gcloud compute project-info describe --project={project_id}",
        "params": ["project_id"],
        "param_hints": {}
      },
      "Describe service quota": {
        "cmd": "gcloud services quota list --service={service} --project={project_id}",
        "params": ["service", "project_id"],
        "param_hints": {}
      }
    }
  },
  "Tags": {
    "🔎 Query": {
      "List tags": {
        "cmd": "gcloud tags list --parent={parent}",
        "params": ["parent"],
        "param_hints": {}
      }
    },
    "📤 Creation": {
      "Create tag key": {
        "cmd": "gcloud tags keys create {tag_key} --parent={parent}",
        "params": ["tag_key", "parent"],
        "param_hints": {}
      },
      "Create tag value": {
        "cmd": "gcloud tags values create {tag_value} --parent={tag_key}",
        "params": ["tag_value", "tag_key"],
        "param_hints": {}
      }
    },
    "📝 Modification": {
      "Attach tag binding": {
        "cmd": "gcloud tags bindings create --tag-value={tag_value} --resource={resource}",
        "params": ["tag_value", "resource"],
        "param_hints": {}
      },
      "Delete tag binding": {
        "cmd": "gcloud tags bindings delete --tag-value={tag_value} --resource={resource}",
        "params": ["tag_value", "resource"],
        "param_hints": {}
      }
    }
  },
  "Policies-Constraints": {
    "🔎 Consulta": {
      "Listar políticas de organización (proyecto)": {
        "cmd": "gcloud org-policies list --project={project}",
        "params": ["project"],
        "param_hints": {}
      },
      "Listar políticas de organización (carpeta)": {
        "cmd": "gcloud org-policies list --folder={folder_id}",
        "params": ["folder_id"],
        "param_hints": {}
      },
      "Listar políticas de organización (org)": {
        "cmd": "gcloud org-policies list --organization={org_id}",
        "params": ["org_id"],
        "param_hints": {}
      },
      "Describir constraint (proyecto)": {
        "cmd": "gcloud org-policies describe {constraint} --project={project}",
        "params": ["constraint", "project"],
        "param_hints": {}
      },
      "Describir constraint (carpeta)": {
        "cmd": "gcloud org-policies describe {constraint} --folder={folder_id}",
        "params": ["constraint", "folder_id"],
        "param_hints": {}
      },
      "Describir constraint (org)": {
        "cmd": "gcloud org-policies describe {constraint} --organization={org_id}",
        "params": ["constraint", "org_id"],
        "param_hints": {}
      }
    },
    "📝 Modificación": {
      "Establecer policy desde archivo": {
        "cmd": "gcloud org-policies set-policy {policy_file}",
        "params": ["policy_file"],
        "param_hints": {
          "policy_file": {"type": "file", "extensions": [".yaml", ".yml", ".json"]}
        }
      },
      "Actualizar constraint con reglas": {
        "cmd": "gcloud org-policies set-policy {policy_file} --project={project}",
        "params": ["policy_file", "project"],
        "param_hints": {}
      }
    },
    "❌ Eliminación": {
      "Borrar constraint de proyecto": {
        "cmd": "gcloud org-policies delete {constraint} --project={project}",
        "params": ["constraint", "project"],
        "param_hints": {}
      },
      "Reset constraint a herencia": {
        "cmd": "gcloud org-policies reset {constraint} --project={project}",
        "params": ["constraint", "project"],
        "param_hints": {}
      }
    }
  }
}
