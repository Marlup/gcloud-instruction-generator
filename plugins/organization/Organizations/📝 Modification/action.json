{
  "Set IAM policy (org)": {
    "cmd": "gcloud organizations set-iam-policy {org_id} {policy_file}",
    "params": [
      "org_id",
      "policy_file"
    ],
    "param_hints": {
      "policy_file": {
        "type": "file",
        "extensions": [
          ".json",
          ".yaml"
        ]
      }
    }
  }
}